Domain(dim=3,kind='structured') :: Omega
Space(domain=Omega,kind='h1')   :: V
Field(space=V)                  :: phi 
Function(x,y,z)                 :: f 
Real                            :: alpha 
Real                            :: beta 

b(v::V) := < f * v >_Omega 
a(v::V, u::V) := < alpha * dx(v) * dx(u) + alpha * dy(v) * dy(u) + alpha * dz(v) * dz(u) + beta * v * u >_Omega
